public class Quadruplet{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int N=sc.nextInt();
long A[]=new long[N];
long max=0;
for(int i=0;i<N;i++)
{
A[i]=sc.nextLong();
}

for(int i=0;i<N-3;i++)
{

long maxQuad=A[i]*A[i+1]*A[i+2]*A[i+3];
if(max<maxQuad)
max=maxQuad;
}
}
}
