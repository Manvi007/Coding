public class StringReplaceClass{

     public static void main(String []args){
        String s="Arg7ts56";
        String s1="";
        String s2="";
        for(int i=0;i<s.length();i++)
        {
            s1=s.replaceAll("[7]","");
        }
        for(int i=0;i<s1.length();i++)
            {
            s2=s1.replaceAll("[56]","");
            }
        System.out.println(s2);
     }
}
